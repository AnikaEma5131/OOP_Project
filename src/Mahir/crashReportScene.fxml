<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="583.0" prefWidth="842.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.CrashReportSceneController">
   <children>
      <Label layoutX="135.0" layoutY="92.0" prefHeight="18.0" prefWidth="265.0" text="Enter detected Problems:">
         <font>
            <Font name="Arial" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="crashReportTextField" layoutX="309.0" layoutY="71.0" prefHeight="59.0" prefWidth="333.0" />
      <BarChart fx:id="crashReportPieChart" layoutX="171.0" layoutY="168.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <Button layoutX="412.0" layoutY="143.0" mnemonicParsing="false" onAction="#addCrashInputToChartButtonOnClicked" text="Add to chart" />
   </children>
</AnchorPane>
