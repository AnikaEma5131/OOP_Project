<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="595.0" prefWidth="819.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.DevelopmentProgressSceneController">
   <children>
      <TableView fx:id="taskTableView" layoutX="96.0" layoutY="122.0" prefHeight="380.0" prefWidth="627.0">
        <columns>
          <TableColumn fx:id="upcomingTaskTC" prefWidth="176.0" text="Upcoming Task" />
          <TableColumn fx:id="ongoingTaskTC" prefWidth="223.0" text="Ongoing Task" />
            <TableColumn fx:id="completedTaskTC" prefWidth="225.0" text="Completed Task" />
        </columns>
      </TableView>
      <Label layoutX="169.0" layoutY="36.0" text="Task description:" />
      <TextField fx:id="taskDescriptionTextField" layoutX="275.0" layoutY="32.0" prefHeight="17.0" prefWidth="353.0" />
      <Button fx:id="addTaskButton" layoutX="352.0" layoutY="81.0" mnemonicParsing="false" onAction="#addTaskButtonOnClicked" text="Add to Upcoming Task" />
      <Button fx:id="editTaskButton" layoutX="162.0" layoutY="514.0" mnemonicParsing="false" onAction="#editTaskButtonOnClicked" prefHeight="35.0" prefWidth="100.0" text="Edit Tasks" />
      <Button fx:id="updateTaskButton" layoutX="498.0" layoutY="514.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="108.0" text="Update Table" />
   </children>
</AnchorPane>
